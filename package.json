{
  "name": "@dogstudio/next-starter",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev -p 3001",
    "lint": "eslint --ext .jsx,.js src/ pages/",
    "start": "next start -p 3001",
    "build": "next build",
    "upgrade": "ncu && npm i",
    "i18n:sync": "cd ./public/locales && env-cmd -f $INIT_CWD/.env.local -x locize sync --api-key \\$LOCIZE_API_KEY --project-id \\$LOCIZE_PROJECT_ID && cd $INIT_CWD",
    "i18n:save": "cd ./public/locales && env-cmd -f $INIT_CWD/.env.local -x locize save-missing --api-key \\$LOCIZE_API_KEY --project-id \\$LOCIZE_PROJECT_ID && cd $INIT_CWD"
  },
  "browserslist": [
    "last 2 version",
    "not ie > 0",
    "not bb > 0",
    "not kaios > 0",
    "not baidu > 0",
    "not ie_mob > 0",
    "not op_mini all"
  ],
  "dependencies": {
    "@welldone-software/why-did-you-render": "^6.1.4",
    "boolean": "^3.0.4",
    "classnames": "^2.3.1",
    "immer": "^9.0.2",
    "locize-cli": "^7.6.14",
    "next": "^10.2.3",
    "next-i18next": "^8.3.0",
    "next-images": "^1.7.0",
    "npm-check-updates": "^11.5.13",
    "object-fit-images": "^3.2.4",
    "object-fit-videos": "^1.0.4",
    "prop-types": "^15.7.2",
    "query-string": "^7.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-use": "^17.2.4",
    "sass-mq": "^5.0.1",
    "ua-parser-js": "^0.7.28",
    "use-immer": "^0.5.2",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "autoprefixer": "^10.2.6",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.6",
    "cssnano": "^5.0.4",
    "env-cmd": "^10.1.0",
    "eslint": "^7.27.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-flowtype": "^5.7.2",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "glslify-import": "^3.1.0",
    "glslify-loader": "^2.0.0",
    "sass": "^1.34.0",
    "sass-loader": "10.1.1",
    "style-loader": "^2.0.0"
  }
}
